{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"declaration": true,
		"noImplicitAny": false,
		"removeComments": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"rootDir": ".",
		"sourceMap": true,
		"listFiles": true,
		"newLine": "LF",
		"noEmitOnError": true
	}
}
